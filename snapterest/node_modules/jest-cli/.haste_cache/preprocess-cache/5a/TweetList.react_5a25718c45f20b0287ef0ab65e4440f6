'use strict';var React = require('react');
var Tweet = require('./Tweet.react');

var listStyle = { 
    padding: '0' };


var listItemStyle = { 
    display: 'inline-block', 
    listStyle: 'none' };


var TweetList = React.createClass({ displayName: 'TweetList', 
    getListOfTweetIds: function getListOfTweetIds() {
        return Object.keys(this.props.tweets);}, 

    getTweetElement: function getTweetElement(tweetId) {
        var tweet = this.props.tweets[tweetId];
        var handleRemoveTweetFromCollection = this.props.onRemoveTweetFromCollection;
        var tweetElement;

        if (handleRemoveTweetFromCollection) {
            tweetElement = 
            React.createElement(Tweet, { 
                tweet: tweet, 
                onImageClick: handleRemoveTweetFromCollection });} else 

        {
            tweetElement = React.createElement(Tweet, { tweet: tweet });}

        return React.createElement('li', { style: listItemStyle, key: tweet.id }, tweetElement);}, 


    render: function render() {
        var tweetElements = this.getListOfTweetIds().map(this.getTweetElement);
        return (
            React.createElement('ul', { style: listStyle }, 
            tweetElements));} });





module.exports = TweetList;